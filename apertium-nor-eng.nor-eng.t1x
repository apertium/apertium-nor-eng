    </def-cat>
    <def-cat n="nom">
      <cat-item tags="n.*"/>
      <cat-item tags="np.*"/>
    </def-cat>
    <def-cat n="adjec">
      <cat-item tags="adj"/>
      <cat-item tags="adj.*"/>
    </def-cat>
    <def-cat n="vinf_pass">
      <cat-item tags="vblex.inf.pass"/>
      <cat-item tags="vblex.inf.pass.*"/>
    </def-cat>
    <def-cat n="vfin_pass">
      <cat-item tags="vblex.pres.pass"/>
      <cat-item tags="vblex.pret.pass"/>
      <cat-item tags="vblex.pres.pass.*"/>
      <cat-item tags="vblex.pret.pass.*"/>
    </def-cat>
    <def-cat n="vfin">
      <cat-item tags="vblex.pres"/>
      <cat-item tags="vblex.pret"/>
      <cat-item tags="vblex.pres.*"/>
      <cat-item tags="vblex.pret.*"/>
    </def-cat>
    <def-cat n="inf">
      <cat-item tags="vblex.inf"/>
      <cat-item tags="vblex.inf.*"/>
    </def-cat>
    <def-cat n="pp">
      <cat-item tags="vblex.pp"/>
      <cat-item tags="vblex.pp.*"/>
    </def-cat>
    <def-cat n="vblex">
      <cat-item tags="vblex.*"/>
    </def-cat>
    <def-cat n="cnjcoo">
      <cat-item tags="cnjcoo"/>
      <cat-item tags="cnjcoo.*"/>
    </def-cat>
    <def-cat n="coma">
      <cat-item tags="cm"/>
      <cat-item tags="cm.*"/>
