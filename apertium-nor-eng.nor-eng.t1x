      <attr-item tags="np"/>
      <attr-item tags="num"/>
      <attr-item tags="abbr"/>
      <attr-item tags="np.al"/>
      <attr-item tags="np.top"/>
      <attr-item tags="np.cog"/>
      <attr-item tags="np.ant"/>
      <attr-item tags="np.org"/>
    </def-attr>
    <def-attr n="grau">
      <attr-item tags="comp"/>
      <attr-item tags="sup"/>
    </def-attr>
    <def-attr n="gen">
      <attr-item tags="m"/>
      <attr-item tags="f"/>
      <attr-item tags="nt"/>
      <attr-item tags="mf"/>
      <attr-item tags="GD"/>
    </def-attr>
    <def-attr n="pers">
      <attr-item tags="p1"/>
      <attr-item tags="p2"/>
      <attr-item tags="p3"/>
      <attr-item tags="PD"/>
    </def-attr>
    <def-attr n="case">
      <attr-item tags="nom"/>
      <attr-item tags="acc"/>
      <attr-item tags="gen"/>
    </def-attr>
    <def-attr n="nbr">
      <attr-item tags="sg"/>
      <attr-item tags="pl"/>
      <attr-item tags="sp"/>
      <attr-item tags="ND"/>
    </def-attr>
    <def-attr n="temps">
      <attr-item tags="pres"/>
      <attr-item tags="past"/>
      <attr-item tags="pri"/>
      <attr-item tags="pret"/>
      <attr-item tags="inf"/>
      <attr-item tags="imp"/>
      <attr-item tags="pp"/>
    </def-attr>
  </section-def-attrs>

  <section-def-vars>
    <def-var n="number"/>
    <def-var n="genere"/>
    <def-var n="np_q"/>
  </section-def-vars>

  <section-def-macros>
    <def-macro n="modify_det" npar="1"
               c="arg.1: det
                  Cleans tags">
      <choose>
        <when>
          <test>
            <equal>
              <clip pos="1" side="tl" part="a_det"/>
              <lit-tag v="adj"/>
            </equal>
          </test>
          <let>
            <clip pos="1" side="tl" part="nbr"/>
            <lit v=""/>
          </let>
        </when>
      </choose>
    </def-macro>

    <def-macro n="modify_adj" npar="1"
               c="arg.1: adj
                  Cleans tags, and may prepend 'more' to lemh
                  Use 'clip whole' when outputting.">
      <let>
        <clip pos="1" side="tl" part="whole"/>
        <concat>
          <clip pos="1" side="tl" part="lemh"/>
          <clip pos="1" side="tl" part="a_adj"/>
          <clip pos="1" side="tl" part="grau"/>
          <clip pos="1" side="tl" part="nbr"/>
          <clip pos="1" side="tl" part="lemq"/>
        </concat>
      </let>
      <choose>
        <when>
          <test>
            <not>
              <or>
                <equal>
                  <clip pos="1" side="tl" part="a_adj"/>
                  <lit-tag v="num.ord"/>
                </equal>
                <equal>
                  <clip pos="1" side="tl" part="a_adj"/>
                  <lit-tag v="det.qnt"/>
                </equal>
              </or>
            </not>
          </test>
          <let>
            <clip pos="1" side="tl" part="nbr"/>
            <lit v=""/>
          </let>
        </when>
        <otherwise>
          <let> <!-- 'mer' â†’ 'more' -->
            <clip pos="1" side="tl" part="grau"/>
            <lit v=""/>
          </let>
        </otherwise>
      </choose>
      <choose>
        <when>
          <test>
            <and>
              <not>
                <equal>
                  <clip pos="1" side="tl" part="a_adj"/>
                  <lit-tag v="adj.sint"/>
                </equal>
              </not>
              <equal>
                <clip pos="1" side="tl" part="a_adj"/>
                <lit-tag v="adj"/>
              </equal>
              <equal>
                <clip pos="1" side="tl" part="grau"/>
                <lit-tag v="comp"/>
              </equal>
            </and>
          </test>
          <let>
            <clip pos="1" side="tl" part="grau"/>
            <lit v=""/>
          </let>
          <let>
            <clip pos="1" side="tl" part="lemh"/>
            <concat>
              <lit v="more"/>
              <lit-tag v="preadv"/>
              <lit v="$ ^"/>
              <clip pos="1" side="tl" part="lemh"/>
            </concat>
          </let>
        </when>
        <when>
          <test>
            <and>
              <not>
                <equal>
                  <clip pos="1" side="tl" part="a_adj"/>
                  <lit-tag v="adj.sint"/>
                </equal>
              </not>
              <equal>
                <clip pos="1" side="tl" part="a_adj"/>
                <lit-tag v="adj"/>
              </equal>
              <equal>
                <clip pos="1" side="tl" part="grau"/>
                <lit-tag v="sup"/>
              </equal>
            </and>
          </test>
          <let>
            <clip pos="1" side="tl" part="grau"/>
            <lit v=""/>
          </let>
          <let>
            <clip pos="1" side="tl" part="lemh"/>
            <concat>
              <lit v="most"/>
              <lit-tag v="preadv"/>
              <lit v="$ ^"/>
              <clip pos="1" side="tl" part="lemh"/>
            </concat>
          </let>
        </when>
      </choose>
    </def-macro>

    <def-macro n="modify_nom" npar="1"
               c="arg.1: nom
                  Cleans tags, and sets np_q if nom is genitive np">
      <let>
        <var n="np_q"/>
        <lit v=""/>
      </let>
      <choose>
        <when>
          <test>
            <not>
              <equal>
                <clip pos="1" side="tl" part="a_nom"/>
                <lit-tag v="np.ant"/>
              </equal>
            </not>
          </test>
          <let>
            <clip pos="1" side="tl" part="gen"/>
            <lit v=""/>
          </let>
        </when>
      </choose>
      <choose>
        <when>
          <test>
            <equal>
              <clip pos="1" side="sl" part="case"/>
              <lit-tag v="gen"/>
            </equal>
          </test>
          <let>
            <var n="np_q"/>
            <concat>
              <lit v="$^'s"/>
              <lit-tag v="gen"/>
            </concat>
          </let>
        </when>
      </choose>
    </def-macro>
  </section-def-macros>

  <section-rules>

    <!-- SECTION: Stranded modifiers -->

    <rule comment="REGLA: det">
      <pattern>
        <pattern-item n="det"/>
      </pattern>      
      <action>
        <call-macro n="modify_det">
          <with-param pos="1"/>
        </call-macro>
        <out>
          <chunk name="det" case="caseFirstWord">
            <tags>
              <tag><lit-tag v="NMOD"/></tag>
            </tags>
            <lu>
              <clip pos="1" side="tl" part="lemh"/>
              <clip pos="1" side="tl" part="a_det"/>
              <clip pos="1" side="tl" part="nbr"/>
              <clip pos="1" side="tl" part="lemq"/>
            </lu>
          </chunk>
        </out>
      </action>
    </rule>

    <!-- SECTION: Sintagmas nominales -->

    <rule comment="REGLA: adjec">
      <pattern>
        <pattern-item n="adjec"/>
      </pattern>
      <action>
        <call-macro n="modify_adj">
          <with-param pos="1"/>
        </call-macro>
        <out>
          <chunk name="adj" case="caseFirstWord">
            <tags>
              <tag><lit-tag v="SA"/></tag>
            </tags>
            <lu>
              <clip pos="1" side="tl" part="whole"/>
            </lu>
          </chunk>
        </out>
      </action>
    </rule>

    <rule comment="REGLA: nom">
      <pattern>
        <pattern-item n="nom"/>
      </pattern>
      <action>
        <call-macro n="modify_nom">
          <with-param pos="1"/>
        </call-macro>
        <out>
          <chunk name="nom" case="caseFirstWord">
            <tags>
              <tag><lit-tag v="SN"/></tag>
              <tag><clip pos="1" side="tl" part="nbr"/></tag>
            </tags>
            <lu>
              <clip pos="1" side="tl" part="lemh"/>
              <clip pos="1" side="tl" part="a_nom"/>
              <clip pos="1" side="tl" part="gen"/>
              <clip pos="1" side="tl" part="nbr" link-to="2"/>
              <clip pos="1" side="tl" part="lemq"/>
              <var n="np_q"/>
            </lu>
          </chunk>
        </out>
        <let>
          <var n="np_q"/>
          <lit v=""/>
        </let>
      </action>
    </rule>

    <rule comment="REGLA: adjec nom">
      <pattern>
        <pattern-item n="adjec"/>
        <pattern-item n="nom"/>
      </pattern>
      <action>
        <call-macro n="modify_adj">
          <with-param pos="1"/>
        </call-macro>
        <call-macro n="modify_nom">
          <with-param pos="2"/>
        </call-macro>
        <out>
          <chunk name="adj_nom" case="caseFirstWord">
            <tags>
              <tag><lit-tag v="SN"/></tag>
              <tag><clip pos="2" side="tl" part="nbr"/></tag>
            </tags>
            <lu>
              <clip pos="1" side="tl" part="whole"/>
            </lu>
            <b pos="1"/>
            <lu>
              <clip pos="2" side="tl" part="lemh"/>
              <clip pos="2" side="tl" part="a_nom"/>
              <clip pos="2" side="tl" part="gen"/>
              <clip pos="2" side="tl" part="nbr" link-to="2"/>
              <clip pos="2" side="tl" part="lemq"/>
              <var n="np_q"/>
            </lu>
          </chunk>
        </out>
        <let>
          <var n="np_q"/>
          <lit v=""/>
        </let>
      </action>
    </rule>

    <rule comment="REGLA: det nom
                   den bilen => that car">
